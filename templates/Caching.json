{
    "name": "Caching",
    "children": [],
    "behaviors": [
        {
            "name": "caching",
            "options": {
                "behavior": "MAX_AGE",
                "mustRevalidate": false,
                "ttl": "${env.caching_ttl}"
            }
        }
    ],
    "criteria": [
        {
            "name": "fileExtension",
            "options": {
                "matchCaseSensitive": false,
                "matchOperator": "IS_ONE_OF",
                "values": [
                    "${env.caching_file}"
                ]
            }    
        }            
            
    ],
    "criteriaMustSatisfy": "all",
    "comments": ""
}